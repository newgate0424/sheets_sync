{
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "env": {
    "DB_HOST": "@db_host",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "DB_NAME": "@db_name",
    "DB_PORT": "@db_port",
    "CRON_SYNC_TOKEN": "@cron_sync_token",
    "NEXT_PUBLIC_CRON_TOKEN": "@next_public_cron_token"
  }
}
