{
  "apps": [{
    "name": "bigquery-app",
    "script": "node_modules/next/dist/bin/next",
    "args": "start -p 3000",
    "cwd": "./",
    "instances": 1,
    "exec_mode": "cluster",
    "watch": false,
    "max_memory_restart": "500M",
    "env": {
      "NODE_ENV": "production",
      "PORT": 3000
    },
    "error_file": "./logs/err.log",
    "out_file": "./logs/out.log",
    "log_file": "./logs/combined.log",
    "time": true
  }]
}
